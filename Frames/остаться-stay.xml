	<roleset id="остаться.01" name="stay, visit">
	  <aliases>
		<alias framenet="" pos="v" verbnet="">оставаться</alias>
		<alias framenet="" pos="j" verbnet="">оставшийся</alias>
		<alias framenet="" pos="j" verbnet="">остающийся</alias>
